QDialog {background-color: #f5f5f5;}
QWidget {background-color: #f5f5f5;}
QLabel {font-size:40px;}
QCheckBox {font-size:40px;spacing:20px;outline-style:none;}
QCheckBox::indicator {Width:30px;Height:30px;}
QPushButton {font-size: 40px;border:none;background-color: transparent;background-repeat:no-repeat;outline-style: none;}
QLineEdit {border: 0px solid gray;font-size:30px;background-color:#E6E6E6;border-radius:5;}
QComboBox {border: 0px solid gray;font-size:30px;color:black;border-top-right-radius: 7px;border-bottom-right-radius: 5px;}
QComboBox QAbstractItemView::item {height: 50px;font-size:30px;}
QComboBox QAbstractItemView {font-size:30px;}
QComboBox::drop-down {width: 51px; border: none;}
QComboBox::down-arrow {width:51px;height:71; padding: 0px 0px 0px 0px;image:url(:/images/xiala.png);}
QComboBox:!editable {color:black}
QTableView {
border: none;
alternate-background-color: rgb(200,200,200);
selection-color: rgb(0,0,0);
}
QTableView::item {border:none}
QTableView::selected {border:none}
QScrollBar:vertical{min-width:30px;}
QScrollBar::handle:vertical {background:rgb(105,105,105);border: 3px solid gray;border-radius:5px;min-height: 50px;min-width:20px;}

QPushButton#btBack {
